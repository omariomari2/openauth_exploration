{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"types": ["./worker-configuration.d.ts"],
		"noEmit": false,
		"outDir": "./dist",
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
